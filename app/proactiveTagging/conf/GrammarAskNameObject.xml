<!-- LANGID 809 for english-UK, 409 for english-US -->

<GRAMMAR  LANGID="809">

  <DEFINE>
    <ID  NAME="object"  VAL="1"/>
  </DEFINE>


  <RULE  NAME="SENTENCEOBJECT"  TOPLEVEL="ACTIVE"  EXPORT="1">

	<P>this is</P>
	<O>
		<L>
			<P>the</P>
			<P>an</P>
			<P>a</P>
			<P>my</P>
		</L>
	</O>
    <RULEREF  NAME="object"/>
   </RULE>



  <RULE NAME="object" DYNAMIC="TRUE">
    <L>
      <P>brain</P>
      <P>crocodile</P>
      <P>mouse</P>
      <P>rabbit</P>
    </L>
  </RULE>

</GRAMMAR>
